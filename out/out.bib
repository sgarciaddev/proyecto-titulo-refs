	pages = {155 – 167},
	doi = {10.1007/978-3-031-29078-7_15},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85174438700&doi=10.1007%2f978-3-031-29078-7_15&partnerID=40&md5=a9b22400a03bf223e404902477c2735f},
