@article{WOS2-9E64508F,
	author = {Sambangi, Swathi and Gondi, Lakshmeeswari},
	title = {Cloud Attacks Detection System for Cloud Load Balancing},
	journal = {INTERNATIONAL JOURNAL OF COMPUTER SCIENCE AND NETWORK SECURITY},
	year = {2022},
	volume = {22},
	number = {5},
	pages = {729-740},
