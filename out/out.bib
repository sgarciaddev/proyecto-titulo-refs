@conference{SCOPUS2-2AF72864,
	author = {Jiao, Lei and Zhou, Ruiting and Lin, Xiaojun and Chen, Xu},
	title = {Online scheduling of trafic diversion and cloud scrubbing with uncertainty in current inputs},
	year = {2019},
	journal = {Proceedings of the International Symposium on Mobile Ad Hoc Networking and Computing (MobiHoc)},
	pages = {271 – 280},
	doi = {10.1145/3323679.3326525},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85069768527&doi=10.1145%2f3323679.3326525&partnerID=40&md5=48fdf2b973291913a239f3ffc6ac63ad},
	affiliations = {Department of Computer and Information Science, University of Oregon, Eugene, OR, United States; School of Cyber Science and Engineering, Wuhan University, Wuhan, China; School of Electrical and Computer Engineering, Purdue University, West Lafayette, IN, United States; School of Data and Computer Science, Sun Yat-sen University, Guangzhou, China},
	abstract = {Operating distributed Scrubbing Centers (SCs) to mitigate massive Distributed Denial of Service (DDoS) traic in large-scale networks faces critical challenges. The operator needs to determine the diversion rule installation and elimination in the networks, as well as the scrubbing resource activation and revocation in the SCs, while minimizing the long-term cost and the cumulative decision-switching penalty without knowing the exact amount of the malicious trafic. We model and formulate this problem as an online nonlinear integer program. In contrast to many other online problems where future inputs are unknown but at least current inputs are known, a key new challenge here is that even part of the current inputs are unknown when decisions are made. To łlearnž the best decisions online, we transform our problem via a gap-preserving approximation into an online optimization problem with only the known inputs, which is further relaxed and decoupled into a series of one-shot convex programs solvable in individual time slots. To overcome the intractability, we design a progressive rounding algorithm to convert fractional decisions into integral ones without violating the constraints. We characterize the competitive ratio of our approach as a function of the key parameters of our problem. We conduct evaluations using real-world data and conirm our algorithms’ superiority over de facto practices and state-of-the-art methods. © 2019 Association for Computing Machinery.},
	author-keywords = {Cloud scrubbing; Competitive analysis; DDoS defense; Online algorithm},
	keywords = {Convex optimization; Denial-of-service attack; Integer programming; Mobile security; Network security; Competitive analysis; Critical challenges; DDoS defense; Distributed denial of service; Nonlinear integer program; On-line algorithms; Online optimization; State-of-the-art methods; Mobile ad hoc networks},
	publisher = {Association for Computing Machinery},
	isbn = {978-145036764-6},
	language = {English},
	abbrev-source-title = {Proc. Int. Symp. Mobile Ad Hoc Networking Comput.},
	type = {Conference paper},
	publication-stage = {Final},
	source = {Scopus},
	note = {Cited by: 2; Conference name: 20th ACM International Symposium on Mobile Ad Hoc Networking and Computing, MobiHoc 2019; Conference date: 2 July 2019 through 5 July 2019; Conference code: 149350; All Open Access, Bronze Open Access}
}


