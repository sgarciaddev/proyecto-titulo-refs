	editor = {Rocha, A and Bordel, B and Penalvo, FG and Goncalves, R},
	title = {IoT security issues in the context of Edge Computing: A Systematic Review of Literature},
	booktitle = {2022 17TH IBERIAN CONFERENCE ON INFORMATION SYSTEMS AND TECHNOLOGIES (CISTI)},
