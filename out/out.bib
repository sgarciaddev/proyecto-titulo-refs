@conference{SCOPUS4-721053C3,
	author = {Sanap, Yogesh B. and Aher, Pushpalata G.},
	title = {Deep Learning Method for Detecting and Mitigating Distributed Denial of Service Attacks with Imbalanced Data},
	year = {2024},
	journal = {2nd International Conference on Integrated Circuits and Communication Systems, ICICACS 2024},
	doi = {10.1109/ICICACS60521.2024.10498918},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85191685172&doi=10.1109%2fICICACS60521.2024.10498918&partnerID=40&md5=f64efa862576b68d7771ab451e61213a},
	affiliations = {Sandip University, Computer Science and Engineering, Nashik, India},
	abstract = {A Distributed Denial-of-Service (DDoS) attack is a malicious attempt to reduce normal traffic service, or network by spreading a target or its associated infrastructure with an internet traffic flood. The salient features of Cloud Computing (CC) are being leveraged by attackers for launching severe DDoS attacks. However, DDoS attacks are very challenging to detect and mitigate the attacks accurately due to their distributed nature. This research proposes the Deep Convolutional Neural Network-based Support Vector Machine Activation Kernel (DCNN-based SVMAK) for detecting and mitigating DDoS attacks accurately using Deep Learning (DL). Initially, the CIC-DDoS2019 and Bot-IoT datasets are utilized to evaluate the proposed technique, and min-max normalization is employed for normalizing the obtained data. Then, the data imbalance issue will be overcome using the Weighted Optimized Synthetic Minority Over-Sampling Technique (WSMOTE). The Hybrid Golden-Seagull optimization (HGSO) is performed by changing sin into cos function for feature selection. The DCNN-based SVMAK is established for the detection and classification of DDoS attacks. If the attack is detected, the BAIT approach is used to mitigate the DDoS. The existing approaches like CNN-Bidirectional Long Short-Term Memory (CNN-BiLSTM), AutoEncoder-Multi-layer Perceptron Network (AE-MLP), Wrapper Feature selection-based Hybrid DL (WF-HDL) are used to compare with the proposed approach. The proposed approach achieves better accuracy of 99.81% and 99.87% for CIC-DDoS2019 and Bot-IoT datasets compared to CNN-BiLSTM, AE-MLP, and WF-HDL respectively. © 2024 IEEE.},
	author-keywords = {Cloud Computing; Deep Learning; Distributed Denial-of-Service; Hybrid Golden-Seagull Optimization; Weighted Optimized Synthetic Minority Over-Sampling Technique},
	keywords = {Cloud computing; Convolutional neural networks; Deep neural networks; Feature extraction; Internet of things; Learning systems; Multilayer neural networks; Network layers; Network security; Support vector machines; Cloud-computing; Convolutional neural network; Deep learning; Denialof- service attacks; Distributed denial of service; Features selection; Hybrid golden-seagull optimization; Optimisations; Synthetic minority over-sampling techniques; Weighted optimized synthetic minority over-sampling technique; Denial-of-service attack},
	correspondence-address = {Y.B. Sanap; Sandip University, Computer Science and Engineering, Nashik, India; email: sanap.yogesh@gmail.com},
	publisher = {Institute of Electrical and Electronics Engineers Inc.},
	isbn = {979-835031755-8},
