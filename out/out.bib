@conference{SCOPUS2-2EC72F09,
	author = {Variant Wahono, Bari Hade and Esti Anggraini, Ratih Nur and Sarno, Riyanarto and Haryono, Agus Tri and Septiyanto, Abdullah Faqih},
	title = {Bot-Net Access Detection System Based on Deep Learning Algorithm in Hybrid-Cloud Infrastructure},
	year = {2024},
	journal = {International Conference on Artificial Intelligence and Mechatronics System, AIMS 2024},
	doi = {10.1109/AIMS61812.2024.10512749},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85193780790&doi=10.1109%2fAIMS61812.2024.10512749&partnerID=40&md5=67505267cefe0fb581cfeffa09864ade},
	affiliations = {Institut Teknologi Sepuluh Nopember, Department of Informatics, Surabaya, Indonesia},
	abstract = {The escalating adoption of hybrid-cloud infrastruc- ture technology across various fields results in a surge in resource utilization, encompassing server instances, databases, microservices, and other hybrid-cloud-based resources, thereby generating diverse log files. These logs serve various purposes, including classifying information based on urgency levels and facilitating data analysis for model training. Moreover, they play a pivotal role in detecting anomalies, such as DDoS attacks, bot-net access, and malware. This study aims to elucidate the process of gathering log files from various sources and processing them to detect bot-net access (formerly known as brute force attacks) across different protocols like SSH, FTP, and Kerberos. One-Hot Encoding is employed for feature extraction to identify bot-net access and distinguish it from other types of access logs. Sub-sequently, Deep Learning algorithms, including Simple Neural Network (SNN), Deep Neural Network (DNN), Artificial Neural Network (ANN), Convolutional Neural Network (CNN), and Long Short- Term Memory (LSTM) are utilized for detection. Our findings reveal that the Convolutional Neural Network (CNN) emerges as the top performer, achieving the highest average accuracy of 86.681 %. These results advance anomaly detection capabilities in hybrid-cloud environments, thereby enhancing overall security measures. © 2024 IEEE.},
	author-keywords = {Arti-ficial Neural Network; Convolutional Neural Network; Deep Neural Network; Long Short-Term Memory; Simple Neural Network},
	keywords = {Anomaly detection; Brain; Classification (of information); Convolution; Convolutional neural networks; Deep neural networks; Denial-of-service attack; Learning algorithms; Malware; Arti-ficial neural network; Cloud infrastructures; Convolutional neural network; Detection system; Hybrid clouds; Logfile; Neural-networks; Simple neural network; Simple++; Long short-term memory},
