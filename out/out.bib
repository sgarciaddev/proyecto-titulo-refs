@conference{SCOPUS2-F5EA60A7,
	author = {Nirmala, E. and Suresh, M. and Maragatharajan, M.},
	title = {Deployment of Robust Security Scheme in SDN Based 5G Network to Detect DDoS Attack},
	year = {2023},
	journal = {Proceedings of the 4th International Conference on Smart Electronics and Communication, ICOSEC 2023},
	pages = {607 – 615},
	doi = {10.1109/ICOSEC58147.2023.10275898},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85175693116&doi=10.1109%2fICOSEC58147.2023.10275898&partnerID=40&md5=a249876b32aad0d2e41969e4fed15ac6},
	affiliations = {School of Computing Science and Engineering, Vit Bhopal University, Madhya Pradesh, Kothrikalan, India},
	abstract = {In the fifth generation of computing, the Cloud Architecture needs rigid security on to its virtualized network by representing the physical units in terms of logical Connections. Network Virtualization in 5G should also provide enhanced security to cloud data against the threat raise by the third party. In the existing schemes, the security mechanisms like detection and prevention of security have been implemented successfully to augment the security. But in 5G networking, when the mobile users are concerned, two features are additionally added to check for the security with the existing feature like Preliminary Authorization, that is validating, before entering into the network. As soon as the task is implemented, handover mechanism is used. In our proposed system the process of preliminary approval and handover is going reduce the latency in the virtualized network mode by Virtualized Handover with Authorization technique (VHAT). In this VHAT scheme the Tree Based Switch Analysis Algorithm (VN-TBSAA) is constructed to check for the packets sent by the sender to the receiver. Moreover, in our proposed TBSAA algorithm takes the flow table of content as input and detects the Distribution of Denial of Service (DDoS) attack is detected, by preventing through redirecting methodology. The overloading happens in case of multiple packets arrival to the same destination point to overcome with this problem in proposed system a methodology called VN-TBSAA is used to reduce attack raised on flow table, to devise it with overloaded entry. this could be avoided via identifying the attacker by assigning the packets to beneath load entry in the flow table. This is going to be implemented by Artificial Neural Network with personal fuzzy identification like biometric or iris. The added advantage of the proposed system is improving the security and reduced time by dropping the fault packets send by them. © 2023 IEEE.},
	author-keywords = {Accuracy; Average time taken; Face Identification; Open CV; Scale Invariant Feature Transform},
	keywords = {5G mobile communication systems; Cloud computing architecture; Network security; Queueing networks; Trees (mathematics); Accuracy; Average time taken; Denialof- service attacks; Face identification; Flow tables; Hand over; Invariant feature transforms; Open CV; Scale invariant feature transform; Scale invariant features; Denial-of-service attack},
	publisher = {Institute of Electrical and Electronics Engineers Inc.},
	isbn = {979-835030088-8},
	language = {English},
