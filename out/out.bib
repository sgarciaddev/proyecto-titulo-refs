	source = {Scopus},
	note = {Cited by: 19; Conference name: 3rd ISEA International Conference on Security and Privacy, ISEA-ISAP 2020; Conference date: 27 February 2020 through 1 March 2020; Conference code: 159571}
}


@article{SCOPUS4-31F11BE2,
	author = {Verma, Priyanka and Tapaswi, Shashikala and Godfrey, W. Wilfred},
